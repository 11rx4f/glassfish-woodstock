<!-- ========== Table Properties =========================================== -->


      <property>
        <property-name>bgcolor</property-name>
        <property-extension>
          <category>APPEARANCE</category>
        </property-extension>
      </property>

      <property>
        <property-name>border</property-name>
        <property-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.IntegerPropertyEditor
          </editor-class>
          <named-value>
            <name>minValue</name>
            <expression>new Integer(0)</expression>
          </named-value>
          <named-value>
            <name>unsetValue</name>
            <expression>new Integer(Integer.MIN_VALUE)</expression>
         </named-value>       
        </property-extension>
      </property>

