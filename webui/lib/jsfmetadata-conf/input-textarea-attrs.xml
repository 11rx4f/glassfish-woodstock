<!-- ========== Input+Textarea Attributes ================================== -->


    <attribute>
      <attribute-name>cols</attribute-name>
      <attribute-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.IntegerPropertyEditor
          </editor-class>
          <named-value>
            <name>minValue</name>
            <expression>new Integer(0)</expression>
          </named-value>
          <named-value>
           <name>unsetValue</name>
           <expression>new Integer(Integer.MIN_VALUE)</expression>
          </named-value>          
      </attribute-extension>
    </attribute>

    <attribute>
      <attribute-name>rows</attribute-name>
      <attribute-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.IntegerPropertyEditor
          </editor-class>
          <named-value>
            <name>minValue</name>
            <expression>new Integer(0)</expression>
          </named-value>
          <named-value>
           <name>unsetValue</name>
           <expression>new Integer(Integer.MIN_VALUE)</expression>
          </named-value>          
      </attribute-extension>
    </attribute>

    <attribute>
      <attribute-name>value</attribute-name>
      <display-name>value (text)</display-name>
      <description>The text value contained and visually presented by this component</description>
    </attribute>
