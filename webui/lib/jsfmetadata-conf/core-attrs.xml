<!-- ========== Core Attributes ============================================ -->


      <attribute>
	<attribute-name>style</attribute-name>
        <attribute-extension>
            <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.css.CssStylePropertyEditor
          </editor-class>
        </attribute-extension>
      </attribute>

      <attribute>
        <attribute-name>styleClass</attribute-name>
        <attribute-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.ChooseManyReferenceDataPropertyEditor
          </editor-class>
          <named-value>
            <name>referenceDataDefiner</name>
            <expression>
              com.sun.jsfcl.std.reference.ReferenceDataManager.STYLE_CLASSES
            </expression>
          </named-value>
        </attribute-extension>
      </attribute>

      <attribute>
        <attribute-name>title</attribute-name>
        <attribute-extension>
            <category>APPEARANCE</category>
        </attribute-extension>
      </attribute>
