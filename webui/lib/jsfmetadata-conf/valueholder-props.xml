<!-- ========== ValueHolder Properties ===================================== -->


      <property>
        <property-name>converter</property-name>
        <property-extension>
            <category>DATA</category>
            <editor-class>
              com.sun.rave.propertyeditors.ConverterPropertyEditor
            </editor-class>
        </property-extension>
      </property>

      <property>
        <property-name>localValue</property-name>
        <property-extension>
          <category>INTERNAL</category>
          <is-hidden>true</is-hidden>
          <read-only>true</read-only>
          <tag-attribute>false</tag-attribute>
        </property-extension>
      </property>

      <property>
        <property-name>value</property-name>
        <property-extension>
            <category>DATA</category>
            <editor-class>
              com.sun.rave.propertyeditors.binding.ValueBindingPropertyEditor
            </editor-class>
            <named-value>
              <name>ignoreIsBound</name>
              <value>true</value>
            </named-value>
        </property-extension>
      </property>

