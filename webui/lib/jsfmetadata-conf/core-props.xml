<!-- ========== Core Properties ============================================ -->


      <property>
	<property-name>style</property-name>
        <property-extension>
            <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.css.CssStylePropertyEditor
          </editor-class>
        </property-extension>
      </property>

      <property>
        <property-name>styleClass</property-name>
        <property-extension>
          <category>APPEARANCE</category>
          <editor-class>
            com.sun.jsfcl.std.property.ChooseManyReferenceDataPropertyEditor
          </editor-class>
          <named-value>
            <name>referenceDataDefiner</name>
            <expression>
              com.sun.jsfcl.std.reference.ReferenceDataManager.STYLE_CLASSES
            </expression>
          </named-value>
        </property-extension>
      </property>

      <property>
        <property-name>title</property-name>
        <property-extension>
            <category>APPEARANCE</category>
        </property-extension>
      </property>
